    <property>
      <code>/* Enables the drag and drop sortable behaviour for entered omim disorders. Uses scriptaculous Sortable. */
function enableDifferentialDragDrop(){
  function makeSortable() {
    var diagnoses = $$('.fieldset.differential_diagnoses .accepted-suggestions')[0];
    if (diagnoses) {
      diagnoses.select('li').each(function(listElement){
        if (listElement.select('.fa-arrows-v').length == 0){
          listElement.insert({ top : '&lt;span class="fa fa-arrows-v"&gt;&lt;/span&gt;'});
          listElement.addClassName('draggable');
        }
      });
      Sortable.create(diagnoses, {
        tag : 'li',
        format: /^.*\.([^.]+)$/,
        constraint: false,
        scroll: window
      });
    } else {
      console.log("Could not find differential diagnoses container element");
    }
  }
  
  makeSortable();
  //Add an event listener for new diagnoses
  var inputField = $$('.fieldset.differential_diagnoses input[name="PhenoTips.PatientClass_0_differential_diagnoses__suggested"]');
  if (inputField.length &gt; 0) {
    document.observe('custom:selection:changed', function(event){
      if(inputField[0].isEqualNode(event.memo.trigger)) {
        makeSortable();
      }
    });
  }
}

document.observe("xwiki:dom:loaded", enableDifferentialDragDrop);</code>
