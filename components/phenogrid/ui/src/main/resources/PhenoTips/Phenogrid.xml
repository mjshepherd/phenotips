  var terms = $('.phenogrid-terms').serializeArray();
  var phenotypes = [];
  terms.each(function(term){
    phenotypes.push(term.value);
  });
  jQuery("#phen_vis").phenogrid({
    // serverURL : "http://monarch.monarchinitiative.org",
    serverURL : "$xwiki.getURL('PhenoTips.PhenogridService', 'get')",
    phenotypeData : phenotypes,
    targetSpecies : "10090"
  });
});
});

